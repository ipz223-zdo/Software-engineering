{
  "compilerOptions": {
    "target": "ESNext",
    "removeComments": false,
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "strict": true,
    "outDir": "dist",
    "noEmitOnError": true,
    "skipLibCheck": true,
    "allowUnreachableCode": true
  },
  "include": [
    "src"
  ]
}

